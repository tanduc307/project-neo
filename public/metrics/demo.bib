@article{ ISI:000356646200008,
Author = {Zhang, Pei and Yan, Fuwu and Du, Changqing},
Title = {{A comprehensive analysis of energy management strategies for hybrid
   electric vehicles based on bibliometrics}},
Journal = {{RENEWABLE \& SUSTAINABLE ENERGY REVIEWS}},
Year = {{2015}},
