	pages = {453--463},
	file = {Lindström et al. - 2010 - The biodiversity of beneficial microbe-host mutual.pdf:D\:\\ZOTERO\\storage\\Q3BJT8JE\\Lindström et al. - 2010 - The biodiversity of beneficial microbe-host mutual.pdf:application/pdf},
