@article{_waste_2014,
	title = {{WASTE} {PRODUCT} ‘{BAGASSE} {ASH}’ {FROM} {SUGAR} {INDUSTRY} {CAN} {BE} {USED} {AS} {STABILIZING} {MATERIAL} {FOR} {EXPANSIVE} {SOILS}},
	volume = {03},
