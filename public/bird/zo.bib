	journal = {ACS Sustainable Chemistry \& Engineering},
	author = {Hwang, Okhwa and Lee, Sang-Ryong and Cho, Sungback and Ro, Kyoung S. and Spiehs, Mindy and Woodbury, Bryan and Silva, Philip J. and Han, Deug-Woo and Choi, Heechul and Kim, Ki-Yong and Jung, Min-Woong},
	month = nov,
