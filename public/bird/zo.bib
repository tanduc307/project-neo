@article{anton-herrero_synergistic_nodate,
	title = {Synergistic effects of biochar and biostimulant on nutrient and toxic element uptake by pepper in contaminated soils},
	volume = {n/a},
	copyright = {This article is protected by copyright. All rights reserved.},
	issn = {1097-0010},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/jsfa.11343},
	doi = {10.1002/jsfa.11343},
	abstract = {Background Nowadays a significant extension of lands contaminated with toxic elements is being used for agriculture, posing a serious risk of crop contamination and toxicity. Several methodologies are being used to remediate soil contamination, including the use of amendments such as biochar. In this work, we evaluated the effects of biochar, combined with different fertirrigations (water, a conventional fertilizer solution, or a fertilizer solution with a commercial biostimulant derived from leonardite) on the availability of toxic elements and nutrients for pepper cultivated in a soil contaminated with As, Cd, Pb and Zn. Results Irrigation with fertilizer solutions improved plant growth regardless biochar amendment. Biochar decreased biovailability of Cu and Pb in soil and Cu content in pepper leaves. Combined with fertilization, biochar also decreased plant As and Pb contents. Biochar combined with biostimulant decreased the bioavailable content of Cd in soil and its uptake by pepper plants. Conclusion The use of biochar and biostimulant posed advantages for plant production in a non-suitable scenario of nutrient scarcity and contamination. This article is protected by copyright. All rights reserved.},
	language = {en},
	number = {n/a},
	urldate = {2021-06-08},
	journal = {Journal of the Science of Food and Agriculture},
	author = {Antón-Herrero, Rafael and Vega-Jara, Liliana and García-Delgado, Carlos and Mayans, Begoña and Camacho-Arévalo, Raquel and Moreno-Jiménez, Eduardo and Plaza, César and Eymar, Enrique},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/jsfa.11343},
	file = {Antón-Herrero et al_Synergistic effects of biochar and biostimulant on nutrient and toxic element.pdf:D\:\\ZOTERO\\storage\\6IRM85RZ\\Antón-Herrero et al_Synergistic effects of biochar and biostimulant on nutrient and toxic element.pdf:application/pdf},
}

