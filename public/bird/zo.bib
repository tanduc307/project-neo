	file = {McLure et al. - 2021 - PoolTestR An R package for estimating prevalence .pdf:D\:\\ZOTERO\\storage\\VES6ZY42\\McLure et al. - 2021 - PoolTestR An R package for estimating prevalence .pdf:application/pdf},
