	year = {2002},
	pages = {203--222},
	file = {Patriarca et al. - 2002 - Key Role of Bacterial NH 4 +.pdf:D\:\\ZOTERO\\storage\\ZS4DEV7V\\Patriarca et al. - 2002 - Key Role of Bacterial NH 4 +.pdf:application/pdf},
