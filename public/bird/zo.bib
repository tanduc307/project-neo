@misc{noauthor_nang_2019,
	title = {Năng lượng liên kết},
	copyright = {Creative Commons Attribution-ShareAlike License},
	url = {https://vi.wikipedia.org/w/index.php?title=N%C4%83ng_l%C6%B0%E1%BB%A3ng_li%C3%AAn_k%E1%BA%BFt&oldid=54908331},
	abstract = {Trong hóa học, năng lượng liên kết (E) hoặc entanpy liên kết (H) là thước đo độ bền liên kết trong liên kết hóa học. Theo IUPAC, năng lượng liên kết là giá trị trung bình của năng lượng phân ly liên kết trong pha khí (thường ở nhiệt độ 298K) cho tất cả các liên kết cùng loại trong cùng loại hóa chất. Theo sách giáo khoa Hóa học 10 của nhà xuất bản giáo dục Việt Nam, năng lượng của một liên kết hoá học là năng lượng cần thiết để phá vỡ liên kết đó và thành nguyên tử ở thể khí. Ví dụ, năng lượng liên kết cacbon–hidro trong metan H(C–H) là sự thay đổi entanpy để phá vỡ một phân tử metan thành một nguyên tử carbon và bốn gốc hydro tự do. Bảng năng lượng liên kết liệt kê các giá trị của năng lượng liên kết trung bình trong phân tử có chứa một số loại liên kết hóa học điển hình. Năng lượng liên kết (E) hoặc entanpy liên kết (H) không nên bị nhầm lẫn với năng lượng phá vỡ liên kết (D). Năng lượng liên kết là trung bình của tất cả các năng lượng phá vỡ liên kết trong một phân tử, và sẽ cho giá trị khác nhau cho một liên kết nhất định so với năng lượng phá vỡ liên kết. Điều này là do năng lượng cần thiết để phá vỡ một liên kết đơn trong một phân tử cụ thể là khác nhau giữa các liên kết trong phân tử đó. Ví dụ, metan có bốn liên kết C–H, năng lượng phá vỡ liên kết D(CH3–H) = 435kJ/mol, D(CH2–H) = 444 kJ/mol, D(CH–H) = 444kJ/mol và D(C–H) = 339 kJ/mol. Vậy năng lượng liên kết trung bình là 414 kJ/mol, giá trị này khác 4 giá trị năng lượng phá vỡ liên kết nêu trên.},
	language = {vi},
	urldate = {2021-01-09},
	journal = {Wikipedia tiếng Việt},
	month = aug,
	year = {2019},
	note = {Page Version ID: 54908331},
	file = {Snapshot:D\:\\ZOTERO\\storage\\QJBQMLVB\\index.html:text/html},
}

