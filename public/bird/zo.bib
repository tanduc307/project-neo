@article{hunter_rhizobium_2007,
	title = {Rhizobium selenireducens sp. nov.: {A} {Selenite}-{Reducing} α-{Proteobacteria} {Isolated} {From} a {Bioreactor}},
	volume = {55},
	issn = {0343-8651, 1432-0991},
