	pages = {51},
	file = {Ngo et al. - 2020 - Feed Preference Response of Weaner Bull Calves to .pdf:D\:\\ZOTERO\\storage\\FT2PD7RX\\Ngo et al. - 2020 - Feed Preference Response of Weaner Bull Calves to .pdf:application/pdf},
