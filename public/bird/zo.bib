	pages = {1547--1552},
	file = {Kayaalp et al. - 2010 - A new approach for chemical oxygen demand (COD) me.pdf:D\:\\ZOTERO\\storage\\CS7PCHEK\\Kayaalp et al. - 2010 - A new approach for chemical oxygen demand (COD) me.pdf:application/pdf},
