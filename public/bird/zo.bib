@article{shepherd_two_2009,
	title = {Two {Dissimilar} {N}-{Acyl}-{Homoserine} {Lactone} {Acylases} of {Pseudomonas} syringae {Influence} {Colony} and {Biofilm} {Morphology}},
	volume = {75},
	issn = {0099-2240, 1098-5336},
	url = {https://AEM.asm.org/content/75/1/45},
	doi = {10.1128/AEM.01723-08},
	abstract = {ABSTRACT
            
              Plant aerial surfaces comprise a complex habitat for microorganisms, and many plant-associated bacteria, such as the pathogen
              Pseudomonas syringae
              , exhibit density-dependent survival on leaves by utilizing quorum sensing (QS). QS is often mediated by diffusible signals called
              N
              -acyl-homoserine lactones (AHLs), and
              P. syringae
              utilizes
