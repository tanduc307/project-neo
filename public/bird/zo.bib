	pages = {1206--1212},
	file = {Michiels et al_1994_Effects of Temperature Stress on Bean-Nodulating Rhizobium Strains.pdf:D\:\\ZOTERO\\storage\\ZIY9C4JH\\Michiels et al_1994_Effects of Temperature Stress on Bean-Nodulating Rhizobium Strains.pdf:application/pdf},
}

