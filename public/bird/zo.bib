	pmcid = {PMC6669334},
	file = {Brown et al_2019_In Silico Modeling of Biofilm Formation by Nontypeable Haemophilus influenzae.pdf:D\:\\ZOTERO\\storage\\5HBRP4ZQ\\Brown et al_2019_In Silico Modeling of Biofilm Formation by Nontypeable Haemophilus influenzae.pdf:application/pdf},
