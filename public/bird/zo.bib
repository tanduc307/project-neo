	abstract = {A multi-trophic, multi-exposure phase assessment approach was applied to characterize the toxicity of sediments collected from two rivers in Guiyu, China, an e-waste recycling centre. Elutriate toxicity tests (bacterium Vibrio ﬁscheri and microalga Selenastrum capricornutum) and whole sediment toxicity test (crustacean Heterocypris incongruens) showed that most sediments exhibited acute toxicity, due to elevated heavy metals and PAHs levels, and low pH caused by uncontrolled acid discharge. The survival rates of crustaceans were negatively (p {\textless} 0.05) correlated with total PAHs in sediments (411–1755 mg kg À1); EC50s of V. ﬁscheri on the elutriates were signiﬁcantly correlated with elutriate pH (p {\textless} 0.01). Signiﬁcant (p {\textless} 0.05) correlations between the induction of hepatic metallothionein in tilapia (Oreochromis mossambicus) and metal concentrations (Cu, Zn, Pb) in sediments were also observed, when ﬁsh were fed with diets containing sediment. The results showed that uncontrolled e-waste recycling activities may bring adverse effects to local aquatic ecosystem.},
	language = {en},
	number = {7},
	urldate = {2021-04-11},
	journal = {Environmental Pollution},
	author = {Wang, F. and Leung, A.O.W. and Wu, S.C. and Yang, M.S. and Wong, M.H.},
	month = jul,
	year = {2009},
	pages = {2082--2090},
	file = {Wang et al. - 2009 - Chemical and ecotoxicological analyses of sediment.pdf:D\:\\ZOTERO\\storage\\V3WWKIFU\\Wang et al. - 2009 - Chemical and ecotoxicological analyses of sediment.pdf:application/pdf},
}

@article{yang_physicochemical_2018,
	title = {Physicochemical {Properties} of {Biochars} {Produced} from {Biosolids} in {Victoria}, {Australia}},
	volume = {15},
	issn = {1660-4601},
	url = {http://www.mdpi.com/1660-4601/15/7/1459},
	doi = {10.3390/ijerph15071459},
	abstract = {Some of the barriers associated with the land application of biosolids generated in wastewater treatment plants can be eliminated simply by converting the biosolids into biochar using a thermal conversion process called “pyrolysis”. In the current work, eight biosolids from four different wastewater treatment plants in southeast Melbourne, Victoria, Australia were collected and pyrolysed to produce biochars at two different temperatures (500 and 700 ◦C). In addition, characterisation studies were carried out on the biochars to obtain their physicochemical properties, which were subsequently compared with the properties of the parent biosolids. The major ﬁndings of the work demonstrated that biochars exhibited large decreases in DTPA-extractable metals such as Cd, Cu, and Zn, and also led to favorable changes in several chemical and physical characteristics (i.e., pH, Olsen P, electrical conductivity, and surface area) for agricultural land application compared to their original form (i.e., biosolids). Overall, the study suggests that there is great potential for converting biosolids to biochar using pyrolysis. This may not only improve the properties of biosolids for land application, but also has potential to reduce the risk to receiving environments and, furthermore, eliminate many of the costly elements associated with biosolids stockpiling and management.},
	language = {en},
	number = {7},
	urldate = {2021-04-11},
	journal = {International Journal of Environmental Research and Public Health},
	author = {Yang, Yumeng and Meehan, Barry and Shah, Kalpit and Surapaneni, Aravind and Hughes, Jeff and Fouché, Leon and Paz-Ferreiro, Jorge},
	month = jul,
	year = {2018},
	pages = {1459},
	file = {ijerph-15-01459.pdf:D\:\\ZOTERO\\storage\\Q7LV9BDR\\ijerph-15-01459.pdf:application/pdf},
}

