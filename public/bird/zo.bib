	file = {Nhut et al. - 2019 - Options to reuse sludge from striped catfish (Pang.pdf:D\:\\ZOTERO\\storage\\N3G973V3\\Nhut et al. - 2019 - Options to reuse sludge from striped catfish (Pang.pdf:application/pdf},
}

