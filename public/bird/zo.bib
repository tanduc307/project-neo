@article{lindstrom_sensitivity_1987,
	title = {Sensitivity of red clover rhizobia to soil acidity factors in pure culture and in symbiosis},
	volume = {98},
	issn = {0032-079X, 1573-5036},
	url = {http://link.springer.com/10.1007/BF02378357},
	doi = {10.1007/BF02378357},
	abstract = {Nineteen Rhizobium trifolii isolates from different parts of Finland were tested for growth on acid laboratory media. The lowest pH at which growth was observed (the critical pH) varied between 4.7 and 4.9 for the strains tested. Five selected strains were also tested for growth in the presence of A1 (5({\textasciitilde}150 {\textasciitilde}M) and Mn (20{\textasciitilde}730 {\textasciitilde}M). A1 and Mn only affected growth at the critical pH level, but not at higher pH values. The five selected strains were also tested for symbiotic properties with red clover (Trifolium pratense) in axenic culture and in a pot experiment with very acidic clayed mud, the pH of which was adjusted to 4.1 (measured in 0.01 M CaC12) and 5.2 by liming. In the pot experiment four of the strains were used as inoculants, whereas the fifth (HAMBI 1344) represented the indigenous rhizobia of the soil used. At pH 5.2, there were no differences between the yields of the inoculated, uninoculated and nitrogen control plants. At pH 4.1, however, inoculation with the best strain improved the yields fourteenfold compared with the uninoculated control, inoculation having a similar effect to the application of lime or mineral nitrogen. There was no correlation between the symbiotic properties of the strains in the very acidic soil and their growth on acid laboratory media. Instead, the pH of the isolation sites of the strains could be used to predict their symbiotic performance in the very acidic soil, the best strain coming from the most acidic isolation site.},
	language = {en},
	number = {3},
	urldate = {2021-11-05},
	journal = {Plant and Soil},
	author = {Lindström, Kristina and Myllyniemi, Heli},
	month = oct,
	year = {1987},
	pages = {353--362},
	file = {Lindström and Myllyniemi - 1987 - Sensitivity of red clover rhizobia to soil acidity.pdf:D\:\\ZOTERO\\storage\\S8XK533D\\Lindström and Myllyniemi - 1987 - Sensitivity of red clover rhizobia to soil acidity.pdf:application/pdf},
}

