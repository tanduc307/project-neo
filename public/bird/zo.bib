	file = {AlMamoory and Al-Mayaly - 2017 - Biodegradation of Cypermethrin by Two Isolates of .pdf:D\:\\ZOTERO\\storage\\ICCELQ79\\AlMamoory and Al-Mayaly - 2017 - Biodegradation of Cypermethrin by Two Isolates of .pdf:application/pdf},
}

@article{jilani_biodegradation_2006,
