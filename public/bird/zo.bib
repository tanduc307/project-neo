@article{yang_cloning_2006,
	title = {Cloning of \textit{mpd} gene from a chlorpyrifos-degrading bacterium and use of this strain in bioremediation of contaminated soil: {Microbial} degradation of organophosphorus pesticides},
	volume = {265},
	issn = {03781097, 15746968},
	shorttitle = {Cloning of mpd gene from a chlorpyrifos-degrading bacterium and use of this strain in bioremediation of contaminated soil},
