	file = {Rodrigues et al. - 2018 - Isolation and prospection of diazotrophic rhizobac.pdf:D\:\\ZOTERO\\storage\\FYDFEQ3F\\Rodrigues et al. - 2018 - Isolation and prospection of diazotrophic rhizobac.pdf:application/pdf},
