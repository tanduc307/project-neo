	file = {Wang et al. - 2015 - Biochar produced from oak sawdust by Lanthanum (La.pdf:D\:\\ZOTERO\\storage\\ZQVBWF43\\Wang et al. - 2015 - Biochar produced from oak sawdust by Lanthanum (La.pdf:application/pdf},
}

@article{vu_removing_2017,
