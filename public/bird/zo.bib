	pages = {153--226},
	file = {Galloway et al. - 2004 - Nitrogen Cycles Past, Present, and Future.pdf:D\:\\ZOTERO\\storage\\SQ7PGZSM\\Galloway et al. - 2004 - Nitrogen Cycles Past, Present, and Future.pdf:application/pdf},
