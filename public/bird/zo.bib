	doi = {10.1021/bk-2008-0991.ch014},
	note = {Section: 14},
	pages = {310--334},
	file = {ACS Full Text Snapshot:D\:\\ZOTERO\\storage\\WQMLHAMY\\bk-2008-0991.html:text/html;Werner_Moran_2008_Effects of Pyrethroid Insecticides on Aquatic Organisms.pdf:D\:\\ZOTERO\\storage\\7EIZ5IC6\\Werner_Moran_2008_Effects of Pyrethroid Insecticides on Aquatic Organisms.pdf:application/pdf},
