	urldate = {2021-01-20},
	journal = {Acta Scientiarum. Agronomy},
	author = {Perin, Lais and Peil, Roberta Marins Nogueira and Höhn, Daniela and Rosa, Douglas Schulz Bergman da and Wieth, Albertina Radtke and Grolli, Paulo Roberto},
