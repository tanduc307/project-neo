@article{velazquez_coexistence_2005,
	title = {The {Coexistence} of {Symbiosis} and {Pathogenicity}-{Determining} {Genes} in \textit{{Rhizobium} rhizogenes} {Strains} {Enables} {Them} to {Induce} {Nodules} and {Tumors} or {Hairy} {Roots} in {Plants}},
